#include <stdio.h>

int main()
{
    int a,b,c,d,e;
  
    printf("Enter 1st number: ");
    scanf("%d",&a);

    printf("Enter 2nd number: ");
    scanf("%d",&b);
  
    printf("Enter 3th number: ");
    scanf("%d",&c);
  
    printf("Enter 4th number: ");
    scanf("%d",&d);

    printf("Enter 5th number: ");
    scanf("%d",&e);
  
    int min = a;
  
    if (b < min) min = b;
    if (c < min) min = c;
    if (d < min) min = d;
    if (e < min) min = e;

    printf("\nMin is %d", min);
  
    return 0;
}
